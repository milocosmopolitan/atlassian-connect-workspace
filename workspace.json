{
  "version": 1,
  "projects": {
    "ac-next-add-on": {
      "root": "apps/ac-next-add-on",
      "sourceRoot": "apps/ac-next-add-on",
      "projectType": "application",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@nrwl/next:build",
          "options": {
            "root": "apps/ac-next-add-on",
            "outputPath": "dist/apps/ac-next-add-on"
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "dist/apps/ac-next-add-on/exported/public/atlassian-connect.json",
                  "with": "apps/ac-next-add-on/environments/atlassian-connect.prod.json"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@nrwl/next:server",
          "options": {
            "buildTarget": "ac-next-add-on:build",
            "dev": true,
            "port": 3000
          },
          "configurations": {
            "production": {
              "buildTarget": "ac-next-add-on:build:production",
              "dev": false
            }
          }
        },
        "export": {
          "builder": "@nrwl/next:export",
          "options": {
            "buildTarget": "ac-next-add-on:build:production"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/ac-next-add-on/**/*.{ts,tsx}"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/ac-next-add-on/jest.config.js",
            "passWithNoTests": true
          }
        },
        "echo": {
          "builder": "./tools/builders/echo:echo",
          "options": {
            "textToEcho": "Hello World"
          }
        },
        "dev": {
          "builder": "./tools/builders/dev:dev",
          "options": {
            "buildTarget": "ac-next-add-on:build",
            "dev": true,
            "port": 3000
          }
        }
      }
    },
    "ac-next-add-on-e2e": {
      "root": "apps/ac-next-add-on-e2e",
      "sourceRoot": "apps/ac-next-add-on-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/ac-next-add-on-e2e/cypress.json",
            "tsConfig": "apps/ac-next-add-on-e2e/tsconfig.e2e.json",
            "devServerTarget": "ac-next-add-on:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "ac-next-add-on:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/ac-next-add-on-e2e/**/*.{js,ts}"]
          }
        }
      }
    },
    "atlassian-connect-dev-tool": {
      "root": "libs/atlassian-connect/dev-tool",
      "sourceRoot": "libs/atlassian-connect/dev-tool/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/atlassian-connect/dev-tool/**/*.ts"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/atlassian-connect/dev-tool/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/next"
  },
  "schematics": {
    "@nrwl/react": {
      "application": {
        "style": "styled-components",
        "linter": "eslint",
        "babel": true
      },
      "component": {
        "style": "styled-components"
      },
      "library": {
        "style": "styled-components",
        "linter": "eslint"
      }
    },
    "@nrwl/next": {
      "application": {
        "style": "styled-components",
        "linter": "eslint"
      }
    }
  },
  "defaultProject": "ac-next-add-on"
}
