{
  "version": 1,
  "projects": {
    "filter-lint": {
      "root": "apps/filter-lint",
      "sourceRoot": "apps/filter-lint/src",
      "projectType": "application",
      "prefix": "filter-lint",
      "schematics": {},
      "architect": {
        "serve": {
          "builder": "@nrwl/workspace:run-commands",
            "options": {
            "commands": [
              { "command": "nx build filter-lint-gadget --baseHref '/filter-lint-gadget/'" },
              { "command": "nx run filter-lint:run-server" }
            ]
          }
        },
        "build": {
          "builder": "@nrwl/node:build",
          "options": {
            "outputPath": "dist/apps/filter-lint",
            "main": "apps/filter-lint/src/main.ts",
            "tsConfig": "apps/filter-lint/tsconfig.app.json",
            "assets": ["apps/filter-lint/src/assets"]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/filter-lint/src/environments/environment.ts",
                  "with": "apps/filter-lint/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "run-server": {
          "builder": "@nrwl/node:execute",
          "options": {
            "buildTarget": "filter-lint:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/filter-lint/**/*.ts"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/filter-lint/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "filter-lint-gadget": {
      "root": "apps/filter-lint-gadget",
      "sourceRoot": "apps/filter-lint-gadget/src",
      "projectType": "application",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@nrwl/web:build",
          "options": {
            "outputPath": "dist/apps/filter-lint/public/filter-lint-gadget",
            "index": "apps/filter-lint-gadget/src/index.html",
            "main": "apps/filter-lint-gadget/src/main.tsx",
            "polyfills": "apps/filter-lint-gadget/src/polyfills.ts",
            "tsConfig": "apps/filter-lint-gadget/tsconfig.app.json",
            "assets": [
              "apps/filter-lint-gadget/src/assets"
            ],
            "styles": [],
            "scripts": [],
            "webpackConfig": "@nrwl/react/plugins/webpack",
            "watch": true
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/filter-lint-gadget/src/environments/environment.ts",
                  "with": "apps/filter-lint-gadget/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@nrwl/web:dev-server",
          "options": {
            "buildTarget": "filter-lint-gadget:build"
          },
          "configurations": {
            "production": {
              "buildTarget": "filter-lint-gadget:build:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/filter-lint-gadget/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/filter-lint-gadget/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "filter-lint-gadget-e2e": {
      "root": "apps/filter-lint-gadget-e2e",
      "sourceRoot": "apps/filter-lint-gadget-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/filter-lint-gadget-e2e/cypress.json",
            "tsConfig": "apps/filter-lint-gadget-e2e/tsconfig.e2e.json",
            "devServerTarget": "filter-lint-gadget:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "filter-lint-gadget:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/filter-lint-gadget-e2e/**/*.{js,ts}"]
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/next"
  },
  "schematics": {
    "@nrwl/react": {
      "application": {
        "style": "styled-components",
        "linter": "eslint",
        "babel": true
      },
      "component": {
        "style": "styled-components"
      },
      "library": {
        "style": "styled-components",
        "linter": "eslint"
      }
    },
    "@nrwl/next": {
      "application": {
        "style": "styled-components",
        "linter": "eslint"
      }
    }
  },
  "defaultProject": "filter-lint"
}
