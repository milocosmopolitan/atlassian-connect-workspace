{
    "key": "hello-world",
    "name": "Nx Workspace for Jira",
    "description": "This is just a tribute...",
    "vendor": {
        "name": "Milo Kang",
        "url": "milowebmaster@gmail.com"
    },
    "baseUrl": "{{localBaseUrl}}",
    "links": {
        "self": "{{localBaseUrl}}/atlassian-connect.json",
        "homepage": "{{localBaseUrl}}/atlassian-connect.json"
    },
    "authentication": {
        "type": "jwt"
    },
    "lifecycle": {
        "installed": "/installed"
