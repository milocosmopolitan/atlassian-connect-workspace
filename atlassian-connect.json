{
    "key": "ac-next-add-on-rc",
    "name": "NextJS Static Plugin - Hello World (RC)",
    "description": "starter",
    "vendor": {
        "name": "Milo Kang",
        "url": "milowebmaster@gmail.com"
    },
    "baseUrl": "{{localBaseUrl}}",
    "links": {
        "self": "{{localBaseUrl}}atlassian-connect.json",
        "homepage": "{{localBaseUrl}}atlassian-connect.json"
    },
    "authentication": {
        "type": "none"
    },
    "enableLicensing": false,
    "lifecycle": {
    },
    "modules": {
        "generalPages": [
            {
                "url" : "/filter-lint-gadget/index.html",
                "name": {
                    "value": "RC: My General Page"
                },
                "key": "my-general-page"
            }
        ],
        "jiraDashboardItems": [
            {
                "name" : {
                  "value" : "RC: Filter Status"
                },
                "url" : "/filter-lint-gadget/index.html?dashboardItemId={dashboardItem.id}&dashboardId={dashboard.id}&view={dashboardItem.viewType}",
                "key" : "filter-lint-gadget",
                "description" : {
                  "value" : "Filter Lint Gadget"
                },
                "thumbnailUrl": "/images/thumbnail.png",
                "configurable" : true
              }
        ]
    },
    "scopes": ["READ", "WRITE"],
    "apiMigrations": {
        "gdpr": false
    }
}
